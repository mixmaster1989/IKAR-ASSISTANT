# Copy to .env and fill with your secrets

# LLM providers
OPENROUTER_API_KEY=your_openrouter_api_key
# Optional additional keys: OPENROUTER_API_KEY2, OPENROUTER_API_KEY3, ...
LLM_API=your_cloud_ru_api_key

# Embeddings / Hugging Face
HF_API_KEY=your_huggingface_api_key
EMBEDDING_API_KEY=your_embeddings_api_key

# Telegram bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Optional: ElevenLabs TTS
ELEVEN_API=your_elevenlabs_api_key
ELEVEN_VOICE_ID=
ELEVEN_MODEL_ID=eleven_multilingual_v2

# Admin API protection (optional). If set, required via Authorization: Bearer, X-Admin-Token or ?token=.
ADMIN_API_TOKEN=change_me

# BingX (optional)
BINGX_API_KEY=
BINGX_SECRET_KEY=
BINGX_TESTNET=false

